<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
  minHeight="-Infinity" minWidth="-Infinity" type="BorderPane" xmlns="http://javafx.com/javafx/11.0.1">
  <center>
    <GridPane BorderPane.alignment="CENTER" hgap="5.0" vgap="5.0">
      <children>
        <Button GridPane.columnIndex="5" GridPane.rowIndex="1" disable="true" mnemonicParsing="false"
          onAction="#fxBtnOpenAction" prefWidth="55.0" text="Abrir" fx:id="fxBtnOpen"/>
        <Button GridPane.columnIndex="5" GridPane.rowIndex="2" disable="true" mnemonicParsing="false"
          onAction="#fxBtnCloseAction" prefWidth="55.0" text="Cerrar" fx:id="fxBtnClose"/>
        <Button GridPane.columnIndex="5" mnemonicParsing="false" onAction="#fxBtnDetailAction" prefWidth="55.0"
          text="Detalle"/>
        <ComboBox GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" prefWidth="150.0" fx:id="fxBoxCaja"/>
        <ComboBox GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
          maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="150.0" fx:id="fxBoxSede"/>
        <Label text="Caja"/>
        <Label GridPane.rowIndex="1" text="Sede"/>
        <Label GridPane.columnIndex="3" text="Estado"/>
        <Label GridPane.columnIndex="3" GridPane.rowIndex="2" text="Cierre"/>
        <Label GridPane.columnIndex="3" GridPane.rowIndex="1" layoutX="250.0" layoutY="52.0" text="Apertura"/>
        <Label GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" maxHeight="1.7976931348623157E308"
          maxWidth="1.7976931348623157E308" text="&lt;status&gt;" fx:id="fxLabelStatus"/>
        <Label GridPane.columnSpan="2" GridPane.rowIndex="2" text="Actividad reciente:"/>
        <TableView GridPane.columnSpan="6" GridPane.rowIndex="3" prefHeight="200.0" fx:id="fxTableCierres">
          <columns>
            <TableColumn prefWidth="75.0" text="ID" fx:id="fxColumnId"/>
            <TableColumn prefWidth="75.0" text="Caja" fx:id="fxColumnCaja"/>
            <TableColumn prefWidth="75.0" text="Apertura" fx:id="fxColumnApertura"/>
            <TableColumn prefWidth="75.0" text="Cierre" fx:id="fxColumnCierre"/>
          </columns>
        </TableView>
        <TextField GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" editable="false"
          maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" fx:id="fxFieldCierre"/>
        <TextField GridPane.columnIndex="4" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" editable="false"
          maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" fx:id="fxFieldApertura"/>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="50.0"/>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="30.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="50.0"/>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="55.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" vgrow="ALWAYS"/>
      </rowConstraints>
    </GridPane>
  </center>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</fx:root>
