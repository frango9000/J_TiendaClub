    <Label GridPane.rowIndex="5" text="direccion"/>
    <Label GridPane.rowIndex="6" text="descripcion"/>
    <Label GridPane.rowIndex="7" layoutX="15.0" layoutY="232.0" text="fechaIn"/>
    <Label GridPane.rowIndex="8" text="activo"/>
    <LocalDateTimeTextField GridPane.columnIndex="1" GridPane.rowIndex="7" allowNull="false" editable="false"
      pickerShowing="true" fx:id="fxPickerFechaIn"/>
    <TextArea style="-fx-wrap-text: true;" GridPane.columnIndex="1" GridPane.rowIndex="5" minHeight="-Infinity"
      prefHeight="40.0" fx:id="fxDireccion">
      <GridPane.margin>
        <Insets/>
