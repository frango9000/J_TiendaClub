      <GridPane.margin>
        <Insets/>
      </GridPane.margin>
    </TextField>
    <TextField fx:id="fxNombre" GridPane.columnIndex="1" GridPane.rowIndex="2">
      <GridPane.margin>
        <Insets/>
      </GridPane.margin>
    </TextField>
    <TextField fx:id="fxTelefono" GridPane.columnIndex="1" GridPane.rowIndex="3">
      <GridPane.margin>
        <Insets/>
      </GridPane.margin>
    </TextField>
    <TextField fx:id="fxEmail" GridPane.columnIndex="1" GridPane.rowIndex="4">
      <GridPane.margin>
        <Insets/>
      </GridPane.margin>
    </TextField>
  </children>
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="NEVER" maxWidth="100.0" minWidth="70.0" prefWidth="70.0"/>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="200.0"/>
