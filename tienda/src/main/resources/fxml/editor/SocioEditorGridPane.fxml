    <CheckBox GridPane.columnIndex="1" GridPane.rowIndex="8" mnemonicParsing="false" fx:id="fxCheckActivo"/>
    <Label text="ID"/>
    <Label GridPane.rowIndex="1" text="DNI"/>
    <Label GridPane.rowIndex="2" text="nombre"/>
    <Label GridPane.rowIndex="3" text="telefono"/>
