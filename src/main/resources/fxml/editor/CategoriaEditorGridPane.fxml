  <children>
    <Label text="ID"/>
    <Label text="Nombre" GridPane.rowIndex="1"/>
    <Label text="activo" GridPane.rowIndex="2"/>
    <TextField fx:id="fxId" editable="false" layoutX="80.0" layoutY="70.0" GridPane.columnIndex="1"/>
    <TextField fx:id="fxNombre" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <CheckBox fx:id="fxCheckActivo" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
