<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11"
  fx:controller="tiendaclub.control.table.SedesPaneControl">
  <center>
    <TableView BorderPane.alignment="CENTER" fx:id="fxTable">
      <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
      </columnResizePolicy>
      <columns>
        <TableColumn maxWidth="50.0" minWidth="20.0" prefWidth="50.0" fx:id="fxColumnIsActive"/>
        <TableColumn editable="false" maxWidth="100.0" minWidth="25.0" prefWidth="100.0" text="Id"
          fx:id="fxColumnId"/>
        <TableColumn editable="false" maxWidth="800.0" prefWidth="-1.0" text="Sede" fx:id="fxColumnSede"/>
        <TableColumn editable="false" maxWidth="800.0" prefWidth="-1.0" text="Telefono"
          fx:id="fxColumnTelefono"/>
        <TableColumn editable="false" maxWidth="800.0" prefWidth="-1.0" text="Direccion"
          fx:id="fxColumnDireccion"/>
      </columns>
    </TableView>
  </center>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
  <right>
    <BorderPane BorderPane.alignment="CENTER">
      <bottom>
        <VBox BorderPane.alignment="CENTER" spacing="10.0">
          <children>
            <Button mnemonicParsing="false" onAction="#fxBtnEliminarAction" prefWidth="80.0"
              text="Eliminar"/>
            <Button mnemonicParsing="false" onAction="#fxBtnRefreshAction" prefWidth="80.0"
              text="Refrescar"/>
            <Button layoutX="10.0" layoutY="45.0" mnemonicParsing="false" onAction="#fxBtnReloadAction"
              prefWidth="80.0" text="Reload"/>
            <Button layoutX="10.0" layoutY="80.0" mnemonicParsing="false" onAction="#fxBtnPullAction"
              prefWidth="80.0" text="Pull"/>
            <Button layoutX="10.0" layoutY="115.0" mnemonicParsing="false" onAction="#fxButtonBackAction"
              prefWidth="80.0" text="Volver"/>
          </children>
        </VBox>
      </bottom>
      <padding>
        <Insets left="10.0"/>
      </padding>
      <top>
        <VBox BorderPane.alignment="CENTER" alignment="TOP_CENTER" spacing="10.0">
          <BorderPane.margin>
            <Insets/>
          </BorderPane.margin>
          <children>
            <Button mnemonicParsing="false" onAction="#fxBtnAddAction" prefWidth="80.0" text="Agregar"/>
            <Button mnemonicParsing="false" onAction="#fxBtnEditAction" prefWidth="80.0" text="Editar"/>
            <Button layoutX="10.0" layoutY="80.0" mnemonicParsing="false" onAction="#fxBtnDisableAction"
              prefWidth="80.0" text="Des/Activar"/>
            <Button mnemonicParsing="false" onAction="#fxBtnShowHideAction" prefWidth="80.0"
              text="Todos" fx:id="fxBtnShowHide"/>
          </children>
        </VBox>
      </top>
    </BorderPane>
  </right>
</BorderPane>
